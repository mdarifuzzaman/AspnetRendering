    <None Include="wwwroot\images\illustration-features-tab-3.svg" />
    <None Include="wwwroot\images\illustration-hero.svg" />
    <None Include="wwwroot\images\logo-bookmark-footer.svg" />
    <None Include="wwwroot\images\logo-bookmark.svg" />
    <None Include="wwwroot\images\logo-chrome.svg" />
    <None Include="wwwroot\images\logo-firefox.svg" />
    <None Include="wwwroot\images\logo-opera.svg" />
  </ItemGroup>
</Project>
